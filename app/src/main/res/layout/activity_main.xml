        app:layout_constraintVertical_bias="0.979"
        app:layout_constraintHorizontal_bias="0.034" />

    <Button
        android:id="@+id/id_btn_relatar"
        android:layout_width="94dp"
        android:layout_height="38dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/str_reportar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.928"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.979" />

    <com.github.anastr.speedviewlib.SpeedView
        android:id="@+id/speedView"
        android:layout_width="247dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25"
        app:sv_cutPadding="0dp"
        app:sv_maxSpeed="90"
        app:sv_minSpeed="0"
        app:sv_speedTextColor="@android:color/background_light"
        app:sv_speedTextSize="17sp"
        app:sv_speedometerMode="TOP"
        app:sv_textColor="@android:color/background_light"
        app:sv_tickNumber="10"
        app:sv_unit="mbps"
        app:sv_unitTextColor="@android:color/background_light" />

</android.support.constraint.ConstraintLayout>
